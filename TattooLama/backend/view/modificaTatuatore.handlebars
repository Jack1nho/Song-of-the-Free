<div class="d-flex">    
    <form method="POST" action="/getTattoers/{{tatuatori.id}}?_method=POST" method="POST" class="list-inline mx-auto justify-content-center">
        <input type="hidden" name="_method" value="POST">
        Nome:<br>
        <input type="text" name="name" class="form-control" value="{{tatuatori.name}}"><br>
        Cognome:<br>
        <input type="text" name="surname" class="form-control" value="{{tatuatori.surname}}"><br>
        Biografia:<br>
        <textarea type="textarea" name="bio" class="form-control" value="{{tatuatori.biography}}"></textarea> <br>
        Citt√†:<br>
        <input type="text" name="city" class="form-control" value="{{tatuatori.city}}"><br>
        Indirizzo:<br>
        <input type="text" name="street" class="form-control" value="{{tatuatori.address}}"><br>
        Telefono:<br>
        <input type="text" name="tele" class="form-control" value="{{tatuatori.telephone}}"><br>
        Email:<br>
        <input type="email" name="email" class="form-control" aria-describedby="emailHelp" class="form-control" value="{{tatuatori.working_email}}"><br>
        Email privata:<br>
        <input type="email" name="private" class="form-control" aria-describedby="emailHelp" class="form-control" value="{{tatuatori.private_email}}"><br>
